{
  "name": "@hebcal/core",
  "version": "3.45.2",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "contributors": [
    "Eyal Schachter (https://github.com/Scimonster)",
    "Danny Sadinoff (https://github.com/dsadinoff)"
  ],
  "keywords": [
    "hebcal",
    "hebrew",
    "calendar",
    "jewish",
    "holidays",
    "chagim",
    "sedra",
    "date",
    "dafyomi",
    "zemanim",
    "zmanim"
  ],
  "description": "A perpetual Jewish Calendar API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "hebcal.d.ts",
  "engines": {
    "node": ">=10.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-es6.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-es6/issues"
  },
  "files": [
    "dist",
    "hebcal.d.ts",
    "po"
  ],
  "scripts": {
    "build:rollup": "rollup -c",
    "build:babel": "babel src --out-dir dist --source-maps",
    "build": "npm run po2json && npm run build:rollup",
    "prepublish": "npm run build",
    "po2json": "node ./po2json.js po/*.po",
    "readme": "npx -p jsdoc-to-markdown jsdoc2md dist/index.js",
    "pretest": "npm run build",
    "typedef": "npx jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/",
    "lint": "eslint src",
    "coverage-local": "nyc --concurrency 2 ava",
    "coverage": "nyc --reporter=lcov ava --concurrency 4 && codecov",
    "test": "ava --concurrency 4"
  },
  "license": "GPL-2.0",
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/register": "^7.18.9",
    "@hebcal/solar-calc": "^1.1.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "ava": "^4.3.3",
    "codecov": "^3.8.3",
    "core-js": "^3.25.5",
    "eslint": "^8.24.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.11",
    "jsdoc-to-markdown": "^7.1.1",
    "nyc": "^15.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "tsd-jsdoc": "^2.5.0",
    "ttag-cli": "^1.10.1"
  }
}
