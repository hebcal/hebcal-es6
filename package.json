{
  "name": "@hebcal/core",
  "version": "3.21.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "contributors": [
    "Eyal Schachter (https://github.com/Scimonster)",
    "Danny Sadinoff (https://github.com/dsadinoff)"
  ],
  "keywords": [
    "hebcal",
    "hebrew",
    "calendar",
    "jewish",
    "holidays",
    "chagim",
    "sedra",
    "date",
    "dafyomi",
    "zemanim",
    "zmanim"
  ],
  "description": "A perpetual Jewish Calendar API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "hebcal.d.ts",
  "engines": {
    "node": ">=10.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-es6.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-es6/issues"
  },
  "files": [
    "dist",
    "hebcal.d.ts"
  ],
  "scripts": {
    "build:rollup": "rollup -c",
    "build:babel": "babel src --out-dir dist --source-maps",
    "build": "npm run po2json && npm run build:rollup",
    "prepublish": "npm run build",
    "po2json": "node ./po2json.js po/*.po",
    "readme": "npx -p jsdoc-to-markdown jsdoc2md dist/index.js",
    "pretest": "npm run build",
    "typedef": "npx jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/",
    "lint": "eslint src",
    "coverage": "nyc ava",
    "test": "ava"
  },
  "license": "GPL-2.0",
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    },
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@hebcal/solar-calc": "^1.0.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "ava": "^3.15.0",
    "core-js": "^3.15.2",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "gematriya": "^2.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.0",
    "nyc": "^15.1.0",
    "rollup": "^2.52.7",
    "rollup-plugin-terser": "^7.0.2",
    "tsd-jsdoc": "^2.5.0",
    "ttag-cli": "^1.9.2"
  }
}
